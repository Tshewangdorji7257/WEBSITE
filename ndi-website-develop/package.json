{
  "name": "ndi-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prod": "npm run build && npm run start",
    "eslint": "eslint . --fix --max-warnings=0",
    "format": "prettier . --write",
    "lint": "next lint",
    "prepare": "husky install",
    "lint-dev": "npm run eslint && npm run format",
    "eslint-dev": "eslint"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write"
    ]
  },
  "include": [
    "./src/**/*"
  ],
  "dependencies": {
    "@apollo/client": "^3.7.15",
