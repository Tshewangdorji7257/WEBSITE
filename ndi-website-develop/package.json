    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "aws-amplify": "^5.2.7",
    "aws-sdk": "^2.1401.0",
    "eslint-config-next": "13.4.4",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.5.0",
    "formik": "^2.4.2",
    "lodash": "^4.17.21",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-alice-carousel": "^2.7.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-markdown": "^8.0.7",
